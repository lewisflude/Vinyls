<%= form_for @vinyl do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %><br/>
  <%= form.label :artist %>
  <%= form.text_field :artist %><br/>
  <%= form.label :genre%>
  <%= form.text_field :genre %><br/>
  <%= form.label :format %>
  <%= form.text_field :format %><br/>
  <%= form.label :rating %>
  <%= form.text_field :rating %><br/>
  <%= form.file_field :album_art %>
  <%= form.submit %>
<% end %>
<%= link_to "Back to Main", vinyls_path %>
