<%= form_for @vinyl do |v| %>
  <%= v.label :title %>
  <%= v.text_field :title %><br/>
  <%= v.label :artist %>
  <%= v.text_field :artist %><br/>
  <%= v.label :genre%>
  <%= v.text_field :genre %><br/>
  <%= v.label :format %>
  <%= v.text_field :format %><br/>
  <%= v.label :rating %>
  <%= v.text_field :rating %><br/>
  <%= v.submit %>
<% end %>
